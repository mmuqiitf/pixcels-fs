{
	"name": "vue-starter-express",
	"version": "0.0.1",
	"description": "Starter template for Vue.js projects with express backend.",
	"main": "server.ts",
	"scripts": {
		"start": "npm run build && ts-node server.ts",
		"build": "webpack --mode production",
		"dev": "concurrently \"webpack --mode development --watch\" \"nodemon --watch server.ts --exec ts-node server.ts\"",
		"type-check": "tsc --noEmit"
	},
	"dependencies": {
		"babel-core": "^6.26.3",
		"babel-preset-es2015": "^6.24.1",
		"express": "^4.16.4",
		"vue": "^2.6.12",
		"vue-router": "^3.6.5"
	},
	"devDependencies": {
		"@babel/core": "^7.18.6",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-proposal-decorators": "^7.24.1",
		"@babel/preset-env": "^7.24.1",
		"@babel/preset-typescript": "^7.24.1",
		"@types/express": "^4.17.13",
		"@types/node": "^16.18.126",
		"@types/vue": "^1.0.31",
		"@vue/cli-plugin-typescript": "^4.5.15",
		"concurrently": "^8.2.2",
		"css-loader": "^6.11.0",
		"nodemon": "^2.0.15",
		"ts-loader": "^9.5.2",
		"ts-node": "^10.4.0",
		"tsify": "^5.0.4",
		"typescript": "^4.5.2",
		"vue-class-component": "^7.2.6",
		"vue-loader": "^15.11.1",
		"vue-property-decorator": "^9.1.2",
		"vue-style-loader": "^4.1.3",
		"vue-template-compiler": "^2.6.12",
		"webpack": "^5.99.8",
		"webpack-cli": "^5.1.4"
	},
	"license": "MIT",
	"keywords": [
		"node",
		"glitch",
		"express",
		"vue"
	]
}
